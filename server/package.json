{
  "name": "scottdbnet-api",
  "version": "1.0.0",
  "author": "Scott Brown",
  "license": "MIT",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon",
    "start:express": "ts-node src/test/runLocalServer.ts",
    "test": "jest",
    "test:ci": "yarn test:types && yarn test:coverage",
    "test:coverage": "jest --coverage",
    "test:types": "tsc --noEmit",
    "test:format": "prettier -l .",
    "validate": "yarn test:format && yarn test:types && yarn test:coverage",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier -w .",
    "config:upload": "aws s3 cp ./src/config/secret.json s3://scottdb-config/scottdbnet/secret.json",
    "config:download": "aws s3 cp s3://scottdb-config/scottdbnet/secret.json ./src/config/secret.json"
  },
  "devDependencies": {
    "@link1900/node-test-util": "0.2.1",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.13",
    "apollo-server-express": "3.5.0",
    "eslint": "8.23.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "jest": "28.1.3",
    "nodemon": "2.0.19",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@link1900/node-environment": "0.3.4",
    "@link1900/node-error": "0.3.1",
    "@link1900/node-express": "0.2.4",
    "@link1900/node-graphql": "0.1.3",
    "@link1900/node-logger": "0.4.3",
    "@link1900/node-script": "0.1.3",
    "@link1900/node-util": "0.7.0",
    "apollo-server-lambda": "3.5.0",
    "class-validator": "0.14.0",
    "express": "4.18.1",
    "graphql": "15.5.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "type-graphql": "1.1.1",
    "typeorm": "0.3.8"
  }
}
