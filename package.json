{
  "name": "scottdbnet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "4.5.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/chance": "1.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "8.3.1",
    "lodash": "4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "6",
    "react-scripts": "4.0.3",
    "react-use": "17.4.0",
    "redux": "4.1.1",
    "redux-persist": "4.9.1",
    "serve": "12.0.0",
    "typescript": "^4.1.2",
    "uuid": "8.3.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "deploy": "yarn build && yarn deploy:build && yarn deploy:index && yarn deploy:invalidate",
    "deploy:build": "aws s3 sync build s3://scottdb.net/ --exclude index.html --cache-control max-age=604800,public",
    "deploy:index": "aws s3 cp build/index.html s3://scottdb.net/index.html --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate --content-type text/html",
    "deploy:invalidate": "aws cloudfront create-invalidation --distribution-id E2ESZP3QQOLMV --paths /",
    "stats": "yarn build -- --stats && npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "start:prod": "yarn build && npx serve -s build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type:check": "tsc --noEmit",
    "validate": "yarn format:check && yarn type:check && yarn test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "4.14.172",
    "prettier": "2.4.1"
  }
}
