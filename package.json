{
  "name": "scottdbnet",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts-ts eject",
    "lint": "tsc --noEmit && tslint src/**/*.tsx",
    "unit-test": "react-scripts-ts test",
    "test": "set CI=true && yarn unit-test",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\" \"src/**/*.ts\" \"src/**/*.tsx\" \"functions/src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "3.7.1",
    "@material-ui/icons": "3.0.1",
    "lodash": "4.17.13",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-redux-firebase": "2.0.5",
    "react-router-dom": "4.2.2",
    "react-scripts-ts": "2.17.0",
    "recompose": "0.27.1",
    "redux": "3.7.2",
    "redux-persist": "4.9.1",
    "shortid": "2.2.14",
    "store": "2.0.12",
    "styled-components": "3.4.5",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/chance": "1.0.1",
    "@types/jest": "23.3.10",
    "@types/lodash": "4.14.119",
    "@types/node": "10.12.18",
    "@types/react": "16.4.11",
    "@types/react-dom": "16.0.7",
    "@types/react-redux": "6.0.6",
    "@types/react-router-dom": "4.3.0",
    "@types/recompose": "0.26.4",
    "@types/shortid": "0.0.29",
    "firebase-tools": "6.10.0",
    "husky": "1.3.1",
    "jest-cli": "23.6.0",
    "lint-staged": "8.1.4",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "source-map-explorer": "1.6.0",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  }
}
